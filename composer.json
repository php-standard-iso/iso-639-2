{
  "name": "php-standard-iso/iso-639-2",
  "type": "library",
  "description": "Standard ISO 639-2 transposed to PHP",
  "license": "BSD-3-Clause",
  "keywords": [
    "iso",
    "international standard organisation",
    "standard",
    "iso639-2",
    "language"
  ],
  "homepage": "https://github.com/php-standard-iso/iso-639-2",
  "support": {
    "email": "github-support@swarmtech.fr"
  },
  "config": {
    "process-timeout": 600,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "gary gitton",
      "homepage": "http://www.github.com/garygitton"
    }
  ],
  "autoload": {
    "psr-4": {
      "Iso639\\Part2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iso639Test\\Part2\\": "test/"
    }
  },
  "require": {
    "php": "^7.3 || ~8.0.0"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "^2.3",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.15.1",
    "vimeo/psalm": "^4.5.2"
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "static-analysis": "psalm --shepherd --stats"
  }
}
